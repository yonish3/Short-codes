const dictionary = {
  "A": ["Aardvark", "Abacus", "Actually", "Atomic"],
  "B": ["Banana", "Bonkers", "Brain", "Bump"],
  "C": ["Callous", "Chain", "Coil", "Czech"]
}

let abc= Object.keys(dictionary)

for (let i = 0; i < abc.length; i++) {

  console.log("Words that begin with "+abc[i]+":")

  for (let k = 0; k < dictionary[abc[i]].length; k++) {
     console.log(dictionary[abc[i]][k])
  }
  
}
